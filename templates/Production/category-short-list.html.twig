<div class="row">    <h2 class="module-title">Catégories de réalisation</h2></div><div class="module-content">    <table class="table">        <caption>Liste des catégories de réalisation</caption>        <thead>        <tr>            <td>Nom</td>            <td>Réalisations associées</td>        </tr>        </thead>        <tbody>        {% for category in productionCategories %}            <tr>                <td class="name-cell">                    {{ category.name }}                </td>                <td class="productions-cell">                    {% for production in category.productions %}<a href="{{ path('cm_back_production_single', {'id': production.id}) }}">{{ production.name }}</a>{% if not loop.last %} | {% endif %}{% else %}<span class="not">Aucune</span>{% endfor %}                </td>            </tr>        {% endfor %}        </tbody>    </table></div><div class="row">    <a href="{{ path('cm_back_production_list', {'_fragment': 'categories'}) }}" class="btn btn-default col-xs-12">Accéder à la section</a></div>